<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MCDiscordBot.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MCDiscordBot"
    Shell.FlyoutBehavior="Flyout"
    
    Shell.BackgroundColor="#000000"
    FlyoutBackground="#171717" 
    FlyoutBackdrop="#7f171717">
    
    
    
    <ShellContent
            Title="Console"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
    <ShellContent
            Title="Config"
            ContentTemplate="{DataTemplate local:Config}"
            Route="Config" />
    
    
    
</Shell>
